<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhang.neteasenews">

    <!-- 网络权限 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".ui.app.NetEaseNewsApp"
        android:allowBackup="true"
        android:icon="@mipmap/zo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activity.LoginActivity" />
        <activity android:name=".ui.activity.HelpAndFBActivity" />
        <activity android:name=".ui.activity.SetActivity" />
        <activity android:name=".ui.activity.secondactivity.VideoActivity" />
        <activity android:name=".ui.activity.secondactivity.NewsDetailActivity" />
        <activity android:name=".ui.activity.secondactivity.NewsDetailVPActivity"></activity>
    </application>

</manifest>